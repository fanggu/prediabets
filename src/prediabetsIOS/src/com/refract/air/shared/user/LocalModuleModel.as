		override public function get downloadPercent():Number { 
			var n:Number = 0;
			if(ProgressByFileSize){
				//in case the reported size in 'meta' isn't correct, check that the number of videos to load != total loaded
				n = (_totalLoaded == _totalVideos && _totalLoaded != 0 ) ? 1 : ((_totalLoadedBytes+_currentFileLoadedBytes)/ModuleInfo.modules["ls"+thisModule].meta.tot_size);
			}else{
				n = (_totalLoaded/_totalVideos);
			}
			return n; 
		}
		
