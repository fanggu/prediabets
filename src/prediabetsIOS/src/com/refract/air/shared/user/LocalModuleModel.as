		private function onListLoaded(evt:Event,loader:URLLoader):void{
			var data:Object = JSON.parse(loader.data);
			Logger.log(Logger.FILE_LOADING,loader.data);
			if(data.hasOwnProperty("success")){
				if(data.hasOwnProperty("data") && data.data.hasOwnProperty("missing")){
					if (NativeAlert.isSupported){
						NativeAlert.show("Some content is missing on the server. Please contact the Lifesaver support team.");
					}
					onError();
					return;
				}else{
					ModuleInfo.modules["ls"+thisModule] = data.data;
					setStoredModuleValue(data.data);
				}
			}
			beginModuleLoad();
		}
		
		private function onNoBackend(evt:Event):void{
			//show an alert, there is no backend.
			if (NativeAlert.isSupported){
				NativeAlert.show("There was an issue connecting to the server. Please check your network connection.");
			}
			
			onError();
		}
		
		private function checkForSpace(videoList:Object):Boolean{
			var total:Number = 0;
			for(var i:int = 0; i < loadList.length; ++i){
				total += int(videoList[loadList[i].name].format.size);
			}
			var space:Number = STORAGE_DIR.spaceAvailable;
			Logger.log(Logger.FILE_LOADING,"TOTAL SPACE: "+total+" REQUIRED SPACE: "+space);
			return total < space;
		}
		
		private function beginModuleLoad():void{
			Logger.log(Logger.FILE_LOADING,"LOADING VIDEOS FOR MODULE "+this.id);
			_downloading = true;
			_totalLoaded = 0;
			_totalLoadedBytes = 0;
			_currentFileLoadedBytes = 0;
			_totalVideos = 0;
			_currentFileLoadedBytes = 0;
			loadList = [];
			var bl:BulkLoader;
			var videoList:Object = ModuleInfo.modules["ls"+thisModule].videos;
			var url:String;
			for(var vid:String in videoList){
				if(!fileExists(vid)){
					url = vid + VideoLoader.VIDEO_FILE_EXT;
					if(BulkLoader.getLoader(vid) && BulkLoader.getLoader(vid).isFinished){
						_totalLoaded++;
						_totalLoadedBytes += int(videoList[vid].format.size);
					}else if(BulkLoader.getLoader(vid)){
						loadList.push(BulkLoader.getLoader(vid));
					}else{
						bl = new BulkLoader(vid);
						var headers:Array = [];
						if(AppSettings.DEBUG){
							headers.push(new URLRequestHeader("Authorization","Basic a2FuaXNoOmVkcDUzd2tu"));
						}
						//bl.add(BackendResponder.BACKEND_DATA_URL+videoList[vid].format.filename,{id:vid, type:BulkLoader.TYPE_BINARY, priority:0,headers:headers});
						bl.addEventListener(BulkLoader.COMPLETE, onModuleLoadComplete);
						bl.addEventListener(BulkLoader.PROGRESS, onModuleProgress);
						bl.addEventListener(BulkLoader.ERROR, onLoadError);
						loadList.push(bl);
					}
				//	bl.start();
				}else{
					_totalLoaded++; //+= int(videoList[vid].format.size);
					_totalLoadedBytes += int(videoList[vid].format.size);
				}
				_totalVideos++;
			}
			
			if(loadList.length == 0){ //length == 0 or _totalLoaded == _totalVideos, same thing
				//videos must be loaded
				_totalLoaded =  _totalVideos; //ModuleInfo.modules["ls"+thisModule].meta.tot_size;
				_totalLoadedBytes = int(videoList.meta.tot_size);
			}else if(checkForSpace(videoList)){
				BulkLoader(loadList[0]).start();
			}else{
				//show alert that there is not enough space.
				if (NativeAlert.isSupported){
					NativeAlert.show("You don't have enough storage space. Please clear some space and try again.");
				}
				onError();
			}
		}
		
		private function onLoadError(evt:ErrorEvent):void{
			//show alert that there is not enough space.
			if (NativeAlert.isSupported){
				NativeAlert.show("There was a problem downloading the module. Please try again.");
			}
			onError();
		}
		
		private function onError():void{
			_downloading = false;
			DispatchManager.dispatchEvent(new MenuEvent(MenuEvent.MENU_LOAD_ERROR,id));
		}
		
		private function onModuleProgress(evt:BulkProgressEvent):void{
			_currentFileLoadedBytes = evt.bytesLoaded;
		//	DispatchManager.dispatchEvent()
		}
		
		private function onModuleLoadComplete(evt:BulkProgressEvent):void{
			var file:File;
			var bl:BulkLoader = evt.currentTarget as BulkLoader;
			bl.removeEventListener(BulkLoader.COMPLETE, onModuleLoadComplete);
			bl.removeEventListener(BulkLoader.PROGRESS, onModuleProgress);
			
			_totalLoaded++;
			_totalLoadedBytes += bl._bytesTotal;
			_currentFileLoadedBytes = 0;
		//	_totalLoaded += int(ModuleInfo.modules["ls"+thisModule].videos[bl.name].format.size);
//			_totalLoaded += bl._bytesTotal;
			if(!fileExists(evt.name)){
				file = new File(VideoLoader.VIDEO_BASE_URL + bl.name + VideoLoader.VIDEO_FILE_EXT);
				if(AppSettings.PLATFORM == AppSettings.PLATFORM_IOS){
					file.preventBackup = true;
				}				
				Logger.log(Logger.FILE_LOADING,"FILE COMPLETE "+bl.name);
				var fs:FileStream = new FileStream();
				fs.open(file,FileMode.WRITE);
				fs.writeBytes(bl.getBinary(bl.name,true));
				fs.close();
				
			}
			var index:int = loadList.indexOf(bl);
			if( index != -1 && index < loadList.length -1 ){
				BulkLoader(loadList[index+1]).start();
			}else{
				Logger.log(Logger.FILE_LOADING,"LOAD COMPLETE FOR MODULE: "+thisModule);
				var resp:String = "/device/"+ 
								  (AppSettings.PLATFORM == AppSettings.PLATFORM_ANDROID ? "android-" : "ios-") +
								  (AppSettings.DEVICE == AppSettings.DEVICE_TABLET ? "tablet" : "mobile") + 
									"/module/"+thisModule+"/download/complete";
				//BackendResponder.apiLog(resp);
				//clear all loaders
			}
		}
		
